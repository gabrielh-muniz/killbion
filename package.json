{
  "name": "ds-killbot",
  "version": "1.0.0",
  "description": "A discord bot build with node.js and discord.js that shows a kill counter for Albion Online players.",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:api": "node --env-file=.env src/services/albion-api.js",
    "dev:gemini": "node --env-file=.env src/services/gemini.js"
  },
  "keywords": [
    "discord",
    "bot",
    "node.js",
    "discord.js",
    "albion online",
    "kill counter"
  ],
  "author": "gabrielh-muniz",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "nodemon": "^3.1.11"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "discord.js": "^14.25.1",
    "pg": "^8.16.3",
    "winston": "^3.18.3"
  }
}
